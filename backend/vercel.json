{
      "version": 2,
      "builds": [
            {
                  "src": "build/src/index.js",
                  "use": "@vercel/node",
                  "config": { "includeFiles": ["build/**"] }
            }
      ],
      "routes": [
            {
                  "src": "/(.*)",
                  "dest": "/"
            }
      ]
}